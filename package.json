{
  "name": "@quesby/core",
  "version": "0.1.1",
  "description": "Core library for Quesby static site builder",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quesby/quesby-core.git"
  },
  "homepage": "https://github.com/quesby/quesby-core#readme",
  "bugs": {
    "url": "https://github.com/quesby/quesby-core/issues"
  },
  "keywords": [
    "quesby",
    "static-site",
    "eleventy",
    "privacy-first",
    "ssg",
    "static-site-generator"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "default": "./index.js"
    },
    "./sass": {
      "import": "./sass/core.scss",
      "default": "./sass/core.scss"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src",
    "sass",
    "index.js"
  ],
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "cheerio": "^1.1.2",
    "dotenv": "^16.6.1",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "markdown-it-link-attributes": "^4.0.1",
    "rehype-expressive-code": "^0.41.3",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "slugify": "^1.6.6",
    "unified": "^11.0.5"
  },
  "peerDependencies": {
    "sass": ">=1.93.3 <2.0.0"
  }
}
