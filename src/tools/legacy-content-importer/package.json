{
    "name": "neutrino-legacy-content-importer",
    "version": "1.0.0",
    "description": "Standalone tool to import legacy markdown content into Neutrino ULID system",
    "main": "index.js",
    "type": "module",
    "bin": {
      "neutrino-import": "./index.js"
    },
  "scripts": {
    "test": "node index.js --help",
    "import": "node index.js",
    "import:neutrino": "node index.js --config neutrino-config.example.yml",
    "import:neutrino:dry": "node index.js --config neutrino-config.example.yml --dry-run"
  },
    "keywords": [
      "neutrino",
      "eleventy",
      "ulid",
      "migration",
      "markdown",
      "content-management"
    ],
    "author": "Neutrino Team",
    "license": "MIT",
    "dependencies": {
      "yargs": "^17.7.2",
      "js-yaml": "^4.1.0"
    }
  }