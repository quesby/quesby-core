#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theoddape/Websites/neutrino-core/node_modules/.pnpm/errno@1.0.0/node_modules/errno/node_modules:/home/theoddape/Websites/neutrino-core/node_modules/.pnpm/errno@1.0.0/node_modules:/home/theoddape/Websites/neutrino-core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/theoddape/Websites/neutrino-core/node_modules/.pnpm/errno@1.0.0/node_modules/errno/node_modules:/home/theoddape/Websites/neutrino-core/node_modules/.pnpm/errno@1.0.0/node_modules:/home/theoddape/Websites/neutrino-core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../errno@1.0.0/node_modules/errno/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../errno@1.0.0/node_modules/errno/cli.js" "$@"
fi
